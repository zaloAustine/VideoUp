<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/video_navigation.xml"
    app:startDestination="@id/videoFeedFragment">

    <fragment
        android:id="@+id/videoFeedFragment"
        android:name="com.zalocoders.videoup2.Views.fragments.VideoFeedFragment"
        android:label="video_feed_fragment"
        tools:layout="@layout/video_feed_fragment" >
        <action
            android:id="@+id/action_videoFeedFragment_to_videoRecordingFragment"
            app:destination="@id/videoRecordingFragment" />
    </fragment>
    <fragment
        android:id="@+id/videoRecordingFragment"
        android:name="com.zalocoders.videoup2.Views.fragments.VideoRecordingFragment"
        android:label="video_recording_fragment"
        tools:layout="@layout/fragment_video_recording" />
</navigation>